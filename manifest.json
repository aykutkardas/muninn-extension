{
  "name": "Muninn Extension",
  "description": "Config Visualizer",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "crow.png"
  },
  "permissions": ["activeTab", "<all_urls>"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
